{
  "name": "pino-seq",
  "version": "2.0.0",
  "description": "A stream that sends Pino log events to Seq",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "test:cli": "node ./test/cli_tests.js | node ./cli.js --property application='pino-seq'",
    "start": "node ./example/example.js",
    "start:ts": "tsx ./example/example.ts"
  },
  "bin": {
