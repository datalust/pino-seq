  "author": "Datalust, Simi Hartstein, and contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/datalust/pino-seq/issues"
  },
  "homepage": "https://github.com/datalust/pino-seq#readme",
  "devDependencies": {
    "mocha": "^11.2.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "seq-logging": "^3.0.0",
    "split2": "^4.2.0",
    "pino": "^9.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
